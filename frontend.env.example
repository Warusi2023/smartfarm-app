# SmartFarm Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL=https://api.smartfarmfiji.com

# Build Configuration
VITE_APP_NAME=SmartFarm
VITE_APP_VERSION=1.0.0
VITE_BUILD_TAG=production

# Feature Flags
VITE_FEATURE_PAYMENTS=true
VITE_FEATURE_ANALYTICS=true
VITE_FEATURE_EMAIL_VERIFICATION=true

# External Services
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_live_publishable_key
VITE_SENTRY_DSN=https://your-frontend-sentry-dsn@sentry.io/project-id

# Analytics
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
VITE_POSTHOG_KEY=your_posthog_key
VITE_POSTHOG_HOST=https://app.posthog.com
